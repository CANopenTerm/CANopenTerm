    <ui:WixUI
      Id="WixUI_InstallDir"
      InstallDirectory="INSTALLFOLDER"
    />

    <WixVariable
      Id="WixUILicenseRtf"
      Value="MIT.rtf"
    />
  </Package>
</Wix>
